{
    "source": "./src",
    "destination": "./docs",
    "plugins": [
        {
            "name": "esdoc-standard-plugin",
            "option": {
                "undocumentIdentifier": {"enable": false},
                "brand": {
                    "title": "django_redux"
                }
            }
        },
        {
            "name": "esdoc-ecmascript-proposal-plugin",
            "option": {
                "classProperties": true,
                "objectRestSpread": true
            }
        },
        {
            "name": "esdoc-importpath-plugin",
            "option": {
                "replaces": [
                  {"from": "^src/", "to": "dist/"},
                  {"from": ".js$", "to": ""}
                ]
            }
        }
    ]
}
